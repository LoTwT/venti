{
  "name": "@ayingott/venti",
  "version": "0.0.32",
  "description": "venti",
  "type": "module",
  "packageManager": "pnpm@8.15.5",
  "author": "LoTwT <709937065@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/venti#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/venti.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/venti/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "bin": {
    "venti": "./dist/bin/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "cli": "tsx ./src/bin/cli.mjs",
    "lint": "eslint .",
    "play": "pnpm -C playground run venti",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "prettier": "prettier --write .",
    "release": "bumpp && npm publish --access public",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "up": "taze latest -I -f",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "defu": "^6.1.4",
    "fs-extra": "^11.2.0",
    "pkg-types": "^1.0.3"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.8.3",
    "@ayingott/prettier-config": "^0.2.2",
    "@ayingott/sucrose": "^0.0.37",
    "@types/envinfo": "^7.8.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.28",
    "@types/prompts": "^2.4.9",
    "@types/rimraf": "^4.0.5",
    "bumpp": "^9.4.0",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "envinfo": "^7.11.1",
    "eslint": "^8.57.0",
    "execa": "^8.0.1",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.5",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.0",
    "taze": "^0.13.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.17.5",
    "vitest": "^1.4.0",
    "yaml": "^2.4.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
