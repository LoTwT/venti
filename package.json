{
  "name": "@ayingott/venti",
  "version": "0.0.10",
  "packageManager": "pnpm@7.13.3",
  "description": "venti",
  "bin": {
    "venti": "./cli.mjs"
  },
  "files": [
    "dist/index.js"
  ],
  "sideEffects": false,
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "lint": "prettier -c .",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish --access public",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I",
    "cli": "node ./cli.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/venti.git"
  },
  "keywords": [],
  "author": "LoTwT <709937065@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LoTwT/venti/issues"
  },
  "homepage": "https://github.com/LoTwT/venti#readme",
  "devDependencies": {
    "@types/envinfo": "^7.8.1",
    "@types/node": "^18.8.3",
    "@types/prompts": "^2.4.1",
    "@types/rimraf": "^3.0.2",
    "bumpp": "^8.2.1",
    "cac": "^6.7.14",
    "chalk": "^5.1.0",
    "envinfo": "^7.8.1",
    "execa": "^6.1.0",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.13.3",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "taze": "^0.8.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "vitest": "^0.24.0",
    "yaml": "^2.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
