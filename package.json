{
  "name": "@ayingott/venti",
  "type": "module",
  "version": "0.0.36",
  "packageManager": "pnpm@9.12.2",
  "description": "venti",
  "author": "LoTwT <709937065@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/venti#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/venti.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/venti/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "bin": {
    "venti": "./dist/bin/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "cli": "tsx ./src/bin/cli.mjs",
    "lint": "eslint .",
    "play": "pnpm -C playground run venti",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "prettier": "prettier --write .",
    "release": "bumpp && npm publish --access public",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "up": "taze latest -I -f",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@antfu/ni": "^0.23.0",
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "defu": "^6.1.4",
    "fs-extra": "^11.2.0",
    "pkg-types": "^1.2.1"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^1.4.7",
    "@ayingott/prettier-config": "^1.2.7",
    "@ayingott/sucrose": "^0.0.37",
    "@types/envinfo": "^7.8.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.7",
    "@types/prompts": "^2.4.9",
    "@types/rimraf": "^4.0.5",
    "bumpp": "^9.7.1",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "envinfo": "^7.14.0",
    "eslint": "^9.13.0",
    "execa": "^9.4.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.17.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "unplugin-auto-import": "^0.18.3",
    "vitest": "^2.1.3",
    "yaml": "^2.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
